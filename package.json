{
  "name": "extension-timer",
  "displayName": "Insert a timer",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.timer"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.timer",
        "title": "Insert a timer"
      }
    ],
    "keybindings": [
      {
        "command": "extension.timer",
        "key": "ctrl+alt+p",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "echo 'No prepublish step needed for JS'",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "vscode-test": "^1.6.1"
  }
}
